<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
                    xmlns:fa="http://schemas.fontawesome.io/icons/"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="TxtBoxFormCreate" x:Name="TxtBoxFromCreate" TargetType="TextBox" BasedOn="{StaticResource ResourceKey=MaterialDesignTextBox}">
        <Setter Property="materialDesign:HintAssist.IsFloating" Value="True" />
    </Style>
    <Style x:Key="BtnReturn" x:Name="BtnReturn" TargetType="Button" BasedOn="{StaticResource ResourceKey=MaterialDesignIconButton}">
        <Setter Property="Content">
            <Setter.Value>
                <fa:FontAwesome Icon="ArrowLeft" />
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>