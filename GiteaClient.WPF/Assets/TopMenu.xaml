<StackPanel x:Class="GiteaClient.WPF.Assets.TopMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:viewModel="clr-namespace:GiteaClient.Core.ViewModels;assembly=GiteaClient.Core"
      xmlns:local="clr-namespace:GiteaClient.WPF.Assets"
      mc:Ignorable="d"
      d:DataContext="{x:Static viewModel:ViewModelBase}"
      d:DesignHeight="450" d:DesignWidth="800"
      Margin="0, 0, 0, 25">

    <Border BorderBrush="DarkGray" BorderThickness="0 0 0 1">
        <StackPanel Orientation="Horizontal" Margin="0, 0, 0, 10">
            <Button Command="{Binding Path=ReturnCommand}" Style="{DynamicResource ResourceKey=BtnReturn}" />
            <Button Command="{Binding Path=ReturnHomeCommand}" Style="{DynamicResource ResourceKey=BtnHome}" />
        </StackPanel>
    </Border>

</StackPanel>
